{
    "name": "gabrielpah7/laravel-sage",
    "description": "Sage php sdk for Laravel",
    "type": "library",
    "license": "Apache-2.0",
    "autoload": {
        "psr-4": {
            "Intacct\\": "src/Intacct/"
        }
      },
    "autoload-dev": {
        "psr-4": {
            "Intacct\\Tests\\": "test/Intacct/"
        }
    },
    "authors": [
        {
            "name": "Gabriel Paladines Herrera",
            "email": "gabriel.herrera@siteimpact.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-dom": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-simplexml": "*",
        "ext-xmlwriter": "*",
        "guzzlehttp/guzzle": "^6.5|^7.0.1",
        "ramsey/uuid": "^4.1.1",
        "psr/log": "^1.1.3",
        "illuminate/support": "^5.1 || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "monolog/monolog": "^2.2.0",
        "mikey179/vfsstream": "^1.6.8",
        "phpmd/phpmd": "^2.9.1",
        "squizlabs/php_codesniffer": "^3.5.8"
    },
    "suggest": {
        "monolog/monolog": "Allows more advanced logging of the application flow"
    }
}
